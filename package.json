  "devDependencies": {
    "@biomejs/biome": "^2.3.0",
    "@types/bun": "^1.3.0",
    "@types/node": "^24.10.4",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.0.4",
    "babel-plugin-react-compiler": "^1.0.0",
    "concurrently": "^9.2.1",
