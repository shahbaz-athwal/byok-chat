    "@convex-dev/better-auth": "^0.10.11",
    "@convex-dev/react-query": "^0.1.0",
    "@radix-ui/react-use-controllable-state": "^1.2.2",
    "@rive-app/react-webgl2": "^4.27.0",
    "@streamdown/cjk": "^1.0.2",
    "@streamdown/code": "^1.0.3",
    "@streamdown/math": "^1.0.2",
    "@streamdown/mermaid": "^1.0.2",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/devtools-vite": "^0.5.1",
    "@tanstack/react-devtools": "^0.9.0",
    "@tanstack/react-query": "^5.90.12",
    "@tanstack/react-router": "^1.159.0",
    "@tanstack/react-router-devtools": "^1.159.1",
    "@tanstack/router-plugin": "^1.132.0",
    "@tanstack/zod-adapter": "^1.159.5",
    "@xyflow/react": "^12.10.1",
    "ai": "5",
    "ansi-to-react": "^6.2.6",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "convex": "^1.31.7",
    "convex-helpers": "^0.1.112",
    "embla-carousel-react": "^8.6.0",
    "lucide-react": "^0.575.0",
    "motion": "^12.34.3",
    "nanoid": "^5.1.6",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-jsx-parser": "^2.4.1",
    "shiki": "^3.22.0",
    "streamdown": "^2.3.0",
