{
  "name": "byok-chat",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently -n \"VITE,CONVEX\" -c \"cyan,magenta\" \"bun run dev:vite\" \"bun run dev:convex\"",
    "dev:vite": "bun --bun vite dev --port 4000",
    "dev:convex": "convex dev",
    "dev:setup": "convex dev --configure --until-success",
    "build": "bun --bun vite build",
    "start": "bun --bun vite preview",
    "lint": "ultracite fix",
    "lint:check": "ultracite check",
    "lint:file": "ultracite fix --",
    "typecheck": "tsc --noEmit && tsc -p convex/tsconfig.json --noEmit"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "2",
    "@ai-sdk/google": "2",
    "@ai-sdk/openai": "2",
    "@base-ui/react": "^1.2.0",
    "@convex-dev/agent": "^0.3.2",
    "@convex-dev/better-auth": "^0.10.11",
    "@convex-dev/react-query": "^0.1.0",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/devtools-vite": "^0.5.1",
    "@tanstack/react-devtools": "^0.9.0",
    "@tanstack/react-query": "^5.90.12",
    "@tanstack/react-router": "^1.159.0",
    "@tanstack/react-router-devtools": "^1.159.1",
    "@tanstack/router-plugin": "^1.132.0",
    "@tanstack/zod-adapter": "^1.159.5",
    "ai": "5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "convex": "^1.31.7",
    "convex-helpers": "^0.1.112",
    "lucide-react": "^0.575.0",
    "motion": "^12.34.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tailwind-merge": "^3.4.0",
    "tw-animate-css": "^1.4.0",
    "usehooks-ts": "^3.1.1",
    "vite-tsconfig-paths": "^6.0.2",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.0",
    "@types/bun": "^1.3.0",
    "@types/node": "^24.10.4",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.0.4",
    "babel-plugin-react-compiler": "^1.0.0",
    "concurrently": "^9.2.1",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "ultracite": "^7",
    "vite": "^7.3.1"
  },
  "engines": {
    "bun": ">=1.3.0"
  },
  "packageManager": "bun@1.3.0"
}
